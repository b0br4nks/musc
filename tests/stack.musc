-- 01_stack.musc
-- "===========================\n" 1 1 sys3 is a separation line

-- push int
1 2 3 => => =>
1 => 2 => 3 =>

"===========================\n" 1 1 sys3

-- push str
"Hello, World\n" 1 1 sys3

"===========================\n" 1 1 sys3

-- duplicate
1 cp => =>
2 cp => cp => =>

"===========================\n" 1 1 sys3

-- pairduplicate
1 2 pcp => => => =>

"===========================\n" 1 1 sys3

-- swap
1 2 ~ => =>

"===========================\n" 1 1 sys3

-- drop
1 2 # =>

"===========================\n" 1 1 sys3

-- over
1 2 over =>

"===========================\n" 1 1 sys3

-- bigint
9999999999999999999 =>
