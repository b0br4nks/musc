-- stack.sko
use <std.sko>

-- push int
1 2 3 => => =>
1 => 2 => 3 =>

"\n" stdout fmt !

-- push str
"Hello, World\n" stdout fmt ! -- ascii
"Привет, Мир\n" stdout fmt ! -- utf-8
"Hello, \"World\"\n" stdout fmt ! -- double quotes

"\n" stdout fmt !

-- multiline str
"Hello,
World
" stdout fmt !

"\n" stdout fmt !

-- character
'X' =>

"\n" stdout fmt !

-- duplicate
1 cp => =>
2 cp => cp => =>

"\n" stdout fmt !

-- pair duplicate
1 2 pcp => => => =>

"\n" stdout fmt !

-- swap
1 2 ~ => =>

"\n" stdout fmt !

-- drop
1 2 ! =>

"\n" stdout fmt !

-- over
1 2 over => !!

"\n" stdout fmt !

-- bigint
9999999999999999999 =>
